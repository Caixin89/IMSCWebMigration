var svg = d3.select('#svg-e-plus');

//cmp
svg.select('#c_cmp')
    .on('mouseover', function() {
        d3.select(this).style('fill', "#990000");
    }).on('mouseout', function() {
    d3.select(this).style('fill', "#777777");
});

svg.select('#t_cmp')
    .on('mouseover', function() {
        svg.select('#c_cmp').style('fill', "#990000");
    }).on('mouseout', function() {
    svg.select('#c_cmp').style('fill', "#777777");
});

